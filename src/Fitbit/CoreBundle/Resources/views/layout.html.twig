<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Fitbit</title>
</head>
<body>

{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
    Logged in as {{ app.user.username }}
    -
    <a href="{{ path('fos_user_security_logout') }}">Logout</a> - <a href="{{ path('fitbit_fitapp_objectives') }}">Objectives</a>
{% else %}
    <a href="{{ path('fos_user_security_login') }}">Login</a>
{% endif %}
<h1>First Layout</h1>

{% for type, messages in app.session.flashBag.all %}
    {% for message in messages %}
        <div class="{{ type }}">
            {{ message|trans({}, 'FOSUserBundle') }}
        </div>
    {% endfor %}
{% endfor %}

<div>
    {% block fos_user_content %}
    {% endblock fos_user_content %}
</div>

{% block body %}
{% endblock %}



{% block javascript %}
{% endblock %}

</body>
</html>